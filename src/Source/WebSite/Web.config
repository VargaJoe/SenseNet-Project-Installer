  <unity>
    <typeAliases>
      <!-- Provider aliases -->
      <typeAlias alias="IApplicationCache" type="SenseNet.ContentRepository.Storage.AppModel.IApplicationCache, SenseNet.Storage" />
      <!--<typeAlias alias="IActionFactory" type="SenseNet.ContentRepository.IActionFactory, SenseNet.ContentRepository" />-->
      <typeAlias alias="ISearchEngine" type="SenseNet.ContentRepository.Storage.Search.ISearchEngine, SenseNet.Storage" />
      <!-- Provider implementation aliases -->
      <typeAlias alias="IApplicationCacheImpl" type="SenseNet.ContentRepository.ApplicationCache, SenseNet.ContentRepository" />
      <!--<typeAlias alias="IActionFactoryImpl" type="SenseNet.Portal.PortalActionLinkManager, SenseNet.Portal" />-->
      <typeAlias alias="ISearchEngineImpl" type="SenseNet.Search.LuceneSearchEngine, SenseNet.ContentRepository" />
      <!--  -->
      <typeAlias alias="MembershipExtenderBase" type="SenseNet.ContentRepository.Storage.Security.MembershipExtenderBase, SenseNet.Storage" />
      <typeAlias alias="MembershipExtender" type="SenseNet.ContentRepository.Storage.Security.DefaultMembershipExtender, SenseNet.Storage" />
      <typeAlias alias="PurgeUrlCollector" type="SenseNet.Portal.Virtualization.PurgeUrlCollector, SenseNet.Portal" />
      <typeAlias alias="IViewManager" type="SenseNet.ContentRepository.IViewManager, SenseNet.ContentRepository" />
      <typeAlias alias="ViewManager" type="SenseNet.Portal.UI.ContentListViews.ViewManager, SenseNet.Portal" />
      <typeAlias alias="singleton" type="Microsoft.Practices.Unity.ContainerControlledLifetimeManager, Microsoft.Practices.Unity" />
    </typeAliases>
    <containers>
      <container name="Providers">
        <types>
          <!-- Binding providers to implementations -->
          <type type="IApplicationCache" mapTo="IApplicationCacheImpl" />
          <!--<type type="IActionFactory" mapTo="IActionFactoryImpl" />-->
          <type type="ISearchEngine" mapTo="ISearchEngineImpl" />
          <!--  -->
          <type type="MembershipExtenderBase" mapTo="MembershipExtender">
            <lifetime type="singleton" />
          </type>
          <type type="PurgeUrlCollector" mapTo="PurgeUrlCollector">
            <lifetime type="singleton" />
          </type>
          <type name="ViewManager" type="IViewManager" mapTo="ViewManager">
            <lifetime type="singleton" />
          </type>
        </types>
      </container>
    </containers>
  </unity>
  <sensenet>
    <webdavSettings>
      <add key="MockExistingFiles" value="desktop.ini,Thumbs.db,wdmaud.drv,foo,MSGRHU32.ini" />
      <add key="WebdavDefaultPageTemplate" value="Internet_Page.html" />
      <!-- Checkout files automatically on opening them in office. Closing the document will invoke a checkin action. -->
      <add key="AutoCheckoutFiles" value="false" />
    </webdavSettings>
    <portalSettings>
      <add key="AllowCssBundling" value="false" />
      <add key="AllowJsBundling" value="false" />
      <!-- Js and Css bundling blacklists: paths placed here (files or folders) will be ignored during bundling and will be referenced individually -->
      <add key="JsBundlingBlacklist" value="/Root/Global/scripts/tinymce/;/Root/Global/scripts/jquery/plugins/tree/" />
      <add key="CssBundlingBlacklist" value="" />
      <add key="EditSourceExtensions" value=".ascx;.asmx;.eml;.config;.css;.js;.xml;.xaml;.html;.htm;.aspx;.template;.xslt;.txt;.ashx;.settings;.cshtml;.json;.vbhtml" />
      <add key="WebdavEditExtensions" value=".doc;.docx;.rtf;.xls;.xlsx;.ppt;.pptx;.xlsm;.xltx;.ods;.odt;.odp;.ppd;.ppsx;.rtf;.mpp" />
      <add key="CheckInComments" value="Recommended" />
      <!--<add key="ProxyIP" value="proxyurl" />
      <add key="PurgeUrlDelayInSeconds" value="5" />-->
      <!--Enable user profiles. If set to TRUE, user profiles will be created for every new user. -->
      <add key="UserProfilesEnabled" value="true" />
      <!--Enable the 'Deny cross-site access' feature. If true, DenyCrossSiteAccess property of sites is in operation. If false, every site is accessible via other sites with absolute addresses. -->
      <add key="DenyCrossSiteAccessEnabled" value="true" />
      <!--Enable download counter. If set to TRUE, the DownloadCounter module will be notified when a file is downloaded. -->
      <!--<add key="DownloadCounterEnabled" value="false" />-->
      <!--Enable portal warmup. If set to true, warmup process starts after the IIS site starts. Default value is TRUE. -->
      <!--<add key="WarmupEnabled" value="true" />-->
      <!--Warmup control query filter. This setting can be used to restrict the control preload scope.-->
      <!--<add key="WarmupControlQueryFilter" value="" />-->
    </portalSettings>
    <dataHandling>
      <add key="BinaryBufferSize" value="1048576" />
      <add key="BinaryChunkSize" value="1048576" />
      <add key="BinaryCacheSize" value="1048576" />
    </dataHandling>
    <!--<formsAuthenticationFromAD>
      <authSettings>
        <authSetting domain="NATIV" adServer="192.168.0.75" virtualADUser="false" />
      </authSettings>
    </formsAuthenticationFromAD>-->
    <!--<urlList>
      <sites>
        <site path="/Root/Sites/Default_Site">
          <urls>
            <url host="localhost" auth="Forms" />
          </urls>
        </site>
      </sites>
    </urlList>-->
    <workflow>
      <!-- Polling time in minutes. Default: 10 -->
      <!--<add key="TimerInterval" value="10" />-->
    </workflow>
    <notification>
      <!--**** Enable/Disable notification feature. Default: on (enabled) ****-->
      <!--<add key="MasterSwitch" value="off" />-->
      <!--**** Notification processing ****-->
      <!-- Timer interval in minutes -->
      <add key="TimerInterval" value="5" />
      <!-- See period configurations in the Notification page of the documentation -->
      <!--<add key="Immediately" value="Never" />-->
      <add key="Daily" value="01:00" />
      <add key="Weekly" value="Monday 01:00" />
      <add key="Monthly" value="Every 1, 01:00" />
      <!--**** Notification sending ****-->
      <!-- Send notifications for multiple events in one email per user -->
      <add key="GroupNotificationsByUser" value="true" />
      <add key="NotificationSenderAddress" value="noreply@sensenet.com" />
      <add key="MessageEncoding" value="utf-8" />
      <add key="RetryCount" value="3" />
      <!-- RetryDelay in millisenconds -->
      <add key="RetryDelay" value="2000" />
      <!-- Take count in pieces -->
      <add key="TakeCount" value="20" />
    </notification>
    <contentNaming>
      <!-- Regex pattern defining invalid name characters. Escape (\) character can be used as is (ie.: "[^a-zA-Z0-9.()[\]]"). Pattern must start with '[' and end with ']'. -->
      <add key="InvalidNameCharsPattern" value="[\$&amp;\+,/:;=?@&quot;&lt;&gt;\#%{}|\\^~\[\u005D'’`\*\t\r\n]" />
      <!-- Character that will be included to the place of the invalid characters.  -->
      <add key="ReplacementChar" value="-" />
    </contentNaming>
    <detailedLogger>
      <!-- Allocated line buffer. Default: 10000 -->
      <!--<add key="BufferSize" value="10000" />-->
      <!-- Time between end of previous and start of next writing in milliseconds. Default: 1000 -->
      <!--<add key="WriteToFileDelay" value="1000" />-->
      <!-- Number of writes into the single file. Default: 100 -->
      <!--<add key="MaxWritesInOneFile" value="99" />-->
      <!-- Frequency of the tracing. Default: 1000 -->
      <!--<add key="LinesPerTrace" value="1000" />-->
    </detailedLogger>
    <cryptography>
      <add key="CertificateThumbprint" value="" />
    </cryptography>
  </sensenet>
  <uri>
    <idn enabled="All" />
    <iriParsing enabled="true" />
  </uri>
  <appSettings>
    <add key="DisabledNodeObservers" value="SenseNet.ContentRepository.Storage.AppModel.AppCacheInvalidator;SenseNet.ContentRepository.Storage.AppModel.RepositoryEventRouter" />
    <add key="DiskFSSupportMode" value="Prefer" />
    <add key="GlobaFieldControlTemplateEnabled" value="true" />
    <add key="DataProvider" value="SenseNet.ContentRepository.Storage.Data.SqlClient.SqlProvider" />
    <add key="DocumentPreviewProvider" value="AsposePreviewProvider.AsposePreviewProvider" />
    <add key="AccessProvider" value="SenseNet.ContentRepository.Security.UserAccessProvider" />
    
    <!-- Switch on migrating passwords. The security will be worse. -->
    <add key="EnablePasswordHashMigration" value="true" />
    
    <!-- Default lock timeout in milliseconds-->
    <add key="DefaultLockTimeout" value="10000000" />
    <add key="DefaultAuthenticationMode" value="Windows" />
    <add key="DefaultDomain" value="BuiltIn" />
    <add key="DefaultSiteName" value="Default_Site" />
    <add key="CrawlerStart" value="false" />
    <add key="SMTP" value="mail.sn.hu" />
    <add key="SMTPUser" value="" />
    <add key="SMTPPassword" value="" />
    <add key="UploadErrorPage" value="/Root/Sites/Default_Site/Error_Upload" />
    <add key="ErrorPage" value="ErrorPage.html" />
    <add key="AbsoluteExpirationSeconds" value="120" />
    <add key="SlidingExpirationSeconds" value="0" />
    <add key="AdminGroupPathsForLoggedInUserCache" value="/Root/IMS/BuiltIn/Portal/Administrators" />
    <add key="WebRootFiles" value="binaryhandler.ashx;Explore.html;ExploreFrame.html;ExploreTree.aspx;picker.aspx;portlet-preview.aspx;prc.ascx;tinyproxy.ashx;UploadProxy.ashx;vsshandler.ashx" />
    
    <!--=========== Indexing ===========-->
    <add key="EnableOuterSearchEngine" value="true" />
    <!--================================-->    
    <add key="BackwardCompatibilityXmlNamespaces" value="true" />
    <!-- Event Partitions start. A partition value means pieces. -->
    <add key="NodeIdDependencyEventPartitions" value="50" />
    <add key="NodeTypeDependencyEventPartitions" value="50" />
    <add key="PathDependencyEventPartitions" value="50" />
    <add key="PortletDependencyEventPartitions" value="50" />
    <!-- Event Partitions end -->
    <!-- Image resizing start -->
    <add key="ResizedImagesCacheFolder" value="/ResizedImages" />
    <!-- Image resizing end -->
    <add key="SNPickerRowNum" value="20" />
    <add key="SNReferenceGridRowNum" value="5" />
    <!-- Default language for a string resource if no language variant is found for the current language. 
    Leave it empty to display resource info (class, key) for non existing language variants or set it to a language (eg. "en") to display non existing language variants in a default language. -->
    <add key="FallbackCulture" value="en" />
    <!-- Timeout in seconds for extracting text from files. Default is 5. -->
    <add key="TextExtractTimeout" value="300" />
    <!-- ============== MVC =============== -->
    <!-- Prevent .cshtml or .vbhtml files from being directly accessible from a web browser. -->
    <add key="webpages:Enabled" value="false" />
  </appSettings>
  <system.net>
    <mailSettings>
      <smtp deliveryMethod="Network">
        <network host="mail.sn.hu" defaultCredentials="true" />
      </smtp>
    </mailSettings>
  </system.net>
